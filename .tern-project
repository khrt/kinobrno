{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "src/*.js"
  ],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "es_modules": {},
    "modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
